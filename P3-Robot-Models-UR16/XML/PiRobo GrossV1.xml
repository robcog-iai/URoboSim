		<origin xyz="0.0 0 0.6" rpy="0 0 0"/>
	</joint>
	<joint name="base_laser_joint" type="fixed">
		<parent link="base_link"/>
		<child link="base_laser"/>
		<origin xyz="1.4 0 0.2" rpy="0 0 0"/>
	</joint>
	<joint name="upper_base_joint" type="fixed">
