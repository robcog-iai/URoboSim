	</joint>
	<joint name="torso_joint" type="revolute">
		<parent link="upper_base_link"/>
		<child link="torso_link"/>
		<origin xyz="0.0 0 0.55" rpy="0 0 0"/>
		<axis xyz="0 0 1"/>
		<limit lower="-3.1416" upper="3.1416" effort="10" velocity="3"/>
	</joint>
<!--first-->
